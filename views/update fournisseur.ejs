<%- include('header'); -%>

    <div class="form">


            <% test.forEach(function (produc) { %>
                <form method="POST" action="/updatefourni/<%= produc.id %>" class="form">
                    <% }) %>

            <div class="form-group">
                <label>Nom du Fournisseur</label>
                <input placeholder="nom" name="nomfour" class="input" id="nom" type="text" required>
            </div>

            <div class="form-group">
                <label>Mail</label>
                <input placeholder="Mail" name="mail" class="input" id="prenom" type="text" required>
            </div>
            <div class="form-group">
                <label>Tele</label>
                <input placeholder="Tele" name="tele" class="input" id="age" type="number" required>
            </div>                          
            <input class="but" type="submit" value="UPDATE">
        </form>



        <br> <br>
    </div>






</body>

</html>